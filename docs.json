{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "UXPlima Docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Welcome",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "faq",
              "contact"
            ]
          }
        ]
      },
      {
        "tab": "Roblox",
        "groups": [
          {
            "group": "Roblox Assets",
            "pages": [
              "roblox/overview",
              "roblox/assets",
              "roblox/animations",
              "roblox/vfx"
            ]
          }
        ]
      },
      {
        "tab": "Minecraft",
        "groups": [
          {
            "group": "Minecraft Assets",
            "pages": [
              "minecraft/overview",
              "minecraft/models",
              "minecraft/textures",
              "minecraft/shaders"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://uxplima.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/uxplima",
          "icon": "slack"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Website",
        "href": "https://uxplima.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Join Discord",
      "href": "https://discord.gg/uxplima"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/uxplima",
      "github": "https://github.com/uxplima",
      "linkedin": "https://linkedin.com/company/uxplima"
    }
  }
}